{
  "name": "server",
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
  "dev": "ts-node-dev src/index.ts",
  "build": "tsc",
  "start": "node dist/index.js",
  "generate": "drizzle-kit generate:pg",
  "migrate": "node -r ts-node/register src/db/migrate.ts",
  "migrate:prod": "node dist/db/migrate.js",
  "prestart": "npm run migrate:prod",
  "backup": "ts-node src/scripts/backup.ts",
  "reset-db": "tsx src/db/reset.ts"
  },
  "dependencies": {
    "@types/pdfkit": "^0.13.7",
    "argon2": "^0.41.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pdfkit": "^0.15.2",
    "pg": "^8.13.1",
    "stripe": "^17.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.30.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}